{
  "hosting": [
    {
      "target": "marketplace",
      "public": "build",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "predeploy": [
        "firebase target:apply hosting marketplace tchopify"
      ]
    }
  ]
}
